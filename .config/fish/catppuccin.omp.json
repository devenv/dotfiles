{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "os": "#ACB0BE",
        "pink": "#F5BDE6",
        "lavender": "#B7BDF8",
        "blue":  "#8AADF4",
        "wight": "#FFFFFF",
        "text": "#494D64"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "powerline_symbol": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#8800dd",
          "template": " <#fefefe>ﮫ</> {{ .FormattedMs }} ",
          "powerline_symbol": "\ue0b4",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "style": "diamond",
          "properties": {
            "folder_icon": "..\ue5fe..",
            "home_icon": "~",
            "style": "agnoster_short"
          },
          "powerline_symbol": "\ue0b4",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "type": "exit",
          "style": "powerline",
          "powerline_symbol": "\ue0b4",
          "foreground": "#ffffff",
          "background": "#00897b",
          "background_templates": [
            "{{ if gt .Code 0 }}#e91e63{{ end }}"
          ],
          "template": "<#193549></>  ",
          "properties": {
            "always_enabled": false
          }
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf594 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0b4",
          "template": " {{ .HEAD }}",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
